{
  "compilerOptions": {
    "allowJs": true,
    "types": ["node"],
    "checkJs": true,
    "strict": true,
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "noEmit": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
  },
  "include": ["eget.js", "package.json"],
  "exclude": ["node_modules"]
}
