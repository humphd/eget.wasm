{
  "name": "eget.wasm",
  "version": "1.3.4",
  "description": "eget WASM with node.js wrapper",
  "type": "module",
  "main": "eget.js",
  "types": "./types/eget.d.ts",
  "exports": {
    ".": {
      "import": "./eget.js",
      "types": "./types/eget.d.ts"
    }
  },
  "files": [
    "eget.js",
    "eget.wasm",
    "types/"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit --allowJs eget.js",
    "build-wasm": "./build-wasi.sh",
    "generate-types": "tsc eget.js --declaration --allowJs --emitDeclarationOnly --outDir types",
    "build": "npm run build-wasm && npm run generate-types",
    "test": "node --test-force-exit --test test/eget.test.js"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.15.30",
    "typescript": "^5.8.3"
  }
}
